[agent]
<% if @masterserver -%>
  server        = <%= @masterserver %>
<% end -%>
<% if @environment -%>
  environment   = <%= @environment %>
<% end -%>
<% if @report -%>
  report        = <%= @report %>
<% end -%>
<% if @pluginsync -%>
  pluginsync    = <%= @pluginsync %>
<% end -%>
<% if @certname -%>
  certname      = <%= @certname %>
<% end -%>
