[master]
<% if @environmentpath -%>
  environmentpath = <%= @environmentpath %>
<% else -%>
<%   if @manifestdir -%>
  manifestdir   = <%= @manifestdir %>
<%   end -%>
<%   if @manifest -%>
  manifest      = <%= @manifest %>
<%   end -%>
  modulepath    = <%= @modulepath %>
<% end -%>
<% if @reports -%>
  reports       = <%= @reports %>
<% end -%>
<% if @dns_alt_names -%>
  dns_alt_names = <%= @dns_alt_names %>
<% end -%>
<% if @storeconfigs -%>
  storeconfigs   = <%= @storeconfigs %>
<% end -%>
<% if @storeconfigs_backend -%>
  storeconfigs_backend   = <%= @storeconfigs_backend %>
<% end -%>
